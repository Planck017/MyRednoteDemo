<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">

    <!-- 填充颜色 -->
    <solid android:color="@color/teal_700" />

    <!-- 边框颜色和宽度 -->
    <stroke
        android:width="2dp"
        android:color="@color/white" />

    <!-- 圆角半径 -->
    <corners android:radius="8dp" />

    <padding
        android:left="16dp"
        android:top="8dp"
        android:right="16dp"
        android:bottom="8dp" />
</shape>